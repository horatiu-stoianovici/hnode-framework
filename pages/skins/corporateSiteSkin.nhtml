    <script>
    $(document).ready(function() {
        $('.department-item img').unbind('error');
        $('.department-item img').error(function(e) {
            $(this).attr('src', "http://placehold.it/186x94&text=No%20Image");
            return true;
        });

        $('.department-image').unbind('error');
        $('.department-image').error(function(e) {
            $(this).attr('src', "http://placehold.it/186x94&text=No%20Image");
            return true;
        });

        var search1 = new Search({
            brands: true,
            categories: true,
            howTos: true,
            members: true,
            products: true,
            projectIdeas: true,
            liveOrAll: 'live',
            inputElement: '#searchInput',
            offsetX: -120
        });

        var search2 = new Search({
            brands: true,
            categories: true,
            howTos: true,
            members: true,
            products: true,
            projectIdeas: true,
            liveOrAll: 'live',
            inputElement: '#mobilesearch',
            offsetX: 115
        });

        function submittedSearchForm(event) {
            event.preventDefault();
            event.stopPropagation();
            if ($('#searchInput').val()) {
                window.location = '/searchresults?q=' + encodeURIComponent($('#searchInput').val());
            }
        }

        function submittedMobileSearchForm(event) {
            event.preventDefault();
            event.stopPropagation();
            if ($('#mobilesearch').val()) {
                window.location = '/searchresults?q=' + encodeURIComponent($('#mobilesearch').val());
            }
        }

        $('#searchInput').keypress(function(e) {
            if (e.which == 13) {
                submittedSearchForm(e);
            }
        });
        $('#searchSubmitButton').click(function(e) {
            submittedSearchForm(e);
        });

        $('#mobilesearch').keypress(function(e) {
            if (e.which == 13) {
                submittedMobileSearchForm(e);
            }
        });
        $('#mobilesearchButton').click(function(e) {
            submittedMobileSearchForm(e);
        });
        //$('#searchform,#mobilesearchform').submit(function (e) {
        //    e.preventDefault();
        //    e.stopPropagation();
        //    if ($('#searchInput').val()) {
        //        window.location = window.location.origin + '/searchresults?q=' + encodeURIComponent($('#searchInput').val());
        //    }
        //    else if ($('#mobilesearch').val()) {
        //        window.location = window.location.origin + '/searchresults?q=' + encodeURIComponent($('#mobilesearch').val());
        //    }
        //});


        var resultFunction = function(resultType, resultObj) {
            switch (resultType) {
                case 'brand':
                    window.location = '/BrandResults.aspx?BrandID=' + resultObj.objectID;
                    break;

                case 'category':
                    window.location = resultObj.Url; // TODO: window.location = window.location.origin + resultObj.Url
                    break;

                case 'howTo':
                    window.location = resultObj.Url; // TODO: window.location = window.location.origin + resultObj.Url
                    break;

                case 'member':
                    window.location = '/store-locator?memberId=' + resultObj.objectID;
                    break;

                case 'product':
                    window.location = resultObj.Url; //'/ProductDetail.aspx?ContentItemId=' + resultObj.objectID;
                    break;

                case 'projectIdea':
                    window.location = resultObj.Url; // TODO: window.location = window.location.origin + resultObj.Url
                    break;
            }
        }

        search1.onResultClick(resultFunction);
        search2.onResultClick(resultFunction);
    });
    </script>

